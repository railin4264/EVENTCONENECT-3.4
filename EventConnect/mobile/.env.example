# App Configuration
EXPO_PUBLIC_APP_NAME=EventConnect
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_APP_ENV=development

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:5000
EXPO_PUBLIC_API_TIMEOUT=10000

# Google Maps
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
EXPO_PUBLIC_GOOGLE_MAPS_LIBRARIES=places,geometry,drawing

# Push Notifications
EXPO_PUBLIC_EXPO_PROJECT_ID=your-expo-project-id
EXPO_PUBLIC_EXPO_ACCESS_TOKEN=your-expo-access-token

# Cloudinary
EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloud-name
EXPO_PUBLIC_CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# Feature Flags
EXPO_PUBLIC_ENABLE_OFFLINE=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_MAPS=true
EXPO_PUBLIC_ENABLE_CHAT=true
EXPO_PUBLIC_ENABLE_ANALYTICS=true

# Development
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=info